<antlib xmlns:current="ant:current">
  <taskdef name="installer" classname="org.formic.ant.InstallerTask"/>
  <taskdef name="package" classname="org.formic.ant.PackageTask"/>
  <taskdef name="property" classname="org.formic.ant.Property"/>
  <taskdef name="tempdir" classname="org.formic.ant.TempDirTask"/>
  <taskdef name="template" classname="org.formic.ant.TemplateTask"/>
  <taskdef name="unzip" classname="org.formic.ant.UnzipTask"/>

  <typedef name="branch" classname="org.formic.ant.type.Branch"/>
  <typedef name="classpath" classname="org.formic.ant.type.Classpath"/>
  <typedef name="libset" classname="org.formic.ant.type.LibSet"/>
  <typedef name="path" classname="org.formic.ant.type.Path"/>
  <typedef name="step" classname="org.formic.ant.type.Step"/>

  <typedef name="istrue" onerror="ignore" classname="org.formic.ant.condition.IsTrue"/>
  <typedef name="isfalse" onerror="ignore" classname="org.formic.ant.condition.IsFalse"/>
  <typedef name="contains" onerror="ignore" classname="org.formic.ant.condition.Contains"/>
</antlib>
